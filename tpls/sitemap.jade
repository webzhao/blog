doctype xml
urlset(xmlns="http://www.sitemaps.org/schemas/sitemap/0.9")
    url
        loc https://webzhao.me
        priority 1
    each post in posts
        url
            loc https://webzhao.me/#{post.name}.html
            priority 0.8
    each page in pages
        url
            loc https://webzhao.me/#{page.name}.html
            priority 0.8
